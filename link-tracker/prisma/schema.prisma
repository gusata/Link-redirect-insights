generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/@prisma/client"
}


datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Link {
  id       Int    @id @default(autoincrement())
  url      String
  slug     String @unique
  clicks    Int      @default(0)
  createdAt DateTime @default(now())
}


